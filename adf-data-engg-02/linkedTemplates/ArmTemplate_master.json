{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "adf-data-engg-02"
		},
		"ls_adls_datalake_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_datalake'"
		},
		"ls_adls_json_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'ls_adls_json'"
		},
		"ls_AzureKeyVault_properties_typeProperties_baseUrl": {
			"type": "string",
			"defaultValue": "https://sqlserverlogin-details.vault.azure.net/"
		},
		"ls_adls_datalake_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adventureworks0011.dfs.core.windows.net/"
		},
		"ls_adls_json_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://adventureworks0011.dfs.core.windows.net/"
		},
		"ls_onpremise_sqlserver_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "DESKTOP-4GO7QUM\\SQLEXPRESS"
		},
		"ls_onpremise_sqlserver_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "AdventureWorksDW2022"
		},
		"ls_onpremise_sqlserver_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "sa_khaza"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "adf-data-engg-02_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"ls_adls_datalake_accountKey": {
						"value": "[parameters('ls_adls_datalake_accountKey')]"
					},
					"ls_adls_json_accountKey": {
						"value": "[parameters('ls_adls_json_accountKey')]"
					},
					"ls_AzureKeyVault_properties_typeProperties_baseUrl": {
						"value": "[parameters('ls_AzureKeyVault_properties_typeProperties_baseUrl')]"
					},
					"ls_adls_datalake_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_datalake_properties_typeProperties_url')]"
					},
					"ls_adls_json_properties_typeProperties_url": {
						"value": "[parameters('ls_adls_json_properties_typeProperties_url')]"
					},
					"ls_onpremise_sqlserver_properties_typeProperties_server": {
						"value": "[parameters('ls_onpremise_sqlserver_properties_typeProperties_server')]"
					},
					"ls_onpremise_sqlserver_properties_typeProperties_database": {
						"value": "[parameters('ls_onpremise_sqlserver_properties_typeProperties_database')]"
					},
					"ls_onpremise_sqlserver_properties_typeProperties_userName": {
						"value": "[parameters('ls_onpremise_sqlserver_properties_typeProperties_userName')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}